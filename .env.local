ENVIRONMENT_NAME=local
# DATABASE
DB_SQL_URL=root:password@tcp(localhost:3306)/go_todos?charset=utf8mb4&parseTime=True&loc=Local
DB_NAME=go_todos
DB_PORT=3306
DB_HOST=localhost
DB_USER=root
DB_PASS=password
DB_LOG_QUERIES=true
DB_SLOW_THRESHOLD_MS=500
DB_TIMEOUT_SECONDS=5
# REDIS
REDIS_URL=localhost:6379
REDIS_PORT=6379
REDIS_AUTH_RATE_LIMIT_COUNT=5
REDIS_AUTH_RATE_LIMIT_SECONDS=10
REDIS_DISABLE_RATE_LIMIT=true
# SERVER
SERVER_PORT=9000
SERVER_DEBUG=true
SERVER_READ_TIMEOUT=10
SERVER_WRITE_TIMEOUT=5
SERVER_SKIP_LOGS=false
SERVER_SKIP_BODY_DUMP=false
# JWT
JWT_
JWT_REFRESH_
JWT_MIN_SECRET_LENGTH=256
JWT_DURATION_MINS=10
JWT_REFRESH_MINS=10080
JWT_ALGO=HS256
